#!/usr/bin/env bash
if [ -z "$1" ]
then
    surfraw duckduckgo "$1"
fi

if [ "$1" = "google" ]
then

    if [ -z "$2" ]
    then
        surfraw duckduckgo "g! $3"
    fi

    if [ "$2" = "reddit" ]
    then
        surfraw duckduckgo "g! reddit $3"
    fi

    if [ "$2" = "stack" ]
    then
        surfraw duckduckgo "stackoverflow $3"
        surfraw duckduckgo "stackexchange $3"
    fi

    if [ "$2" = "archwiki" ]
    then
        surfraw archwiki "$3"
    fi

    if [ "$2" = "urbandictionary" ]
    then
        surfraw archwiki "$3"
    fi
fi

if [ "$1" = "duckreddit" ]
then
    surfraw duckduckgo "r! $2"
fi

if [ "$1" = "duckstack" ]
then
    surfraw duckduckgo "stackoverflow $2"
    surfraw duckduckgo "stackexchange $2"
fi

if [ "$1" = "youtube" ]
then
    surfraw duckduckgo "yt! $2"
fi

if [ "$1" = "archwiki" ]
then
    surfraw archwiki "$2"
fi

if [ "$1" = "urbandictionary" ]
then
    surfraw archwiki "$2"
fi
